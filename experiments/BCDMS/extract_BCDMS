#!/bin/bash
  
# just want 1 2 6 7 8
echo " x   Q**2   F2(R=0)      errors(stat,sys)  F2(R=QCD)   errors "

aCMD=`echo -e "Create table F2_bcdms ( \nnucleon varchar(10), \n xbjorken double ,\n Qsquared double ,\n F2 double ,\n F2_error_stat double ,\n f2_error_sys double\n )" `
mysql nuclearDB -u bjorken -pdrell -N -e"${aCMD}"



exec 3< extractors/BCDMS/f2protcomb.dat
while read <&3 #### LOOP OVER LINES
do  # use $REPLY

  set -- $REPLY

#  echo $1 $2 $3 $4 $5


aCMD=`echo -e "INSERT INTO F2_bcdms set "`
aCMD=`echo -e "${aCMD} xbjorken=${1}, "`
aCMD=`echo -e "${aCMD} Qsquared=${2}, "	`
aCMD=`echo -e "${aCMD} F2=${6}, F2_error_stat=${7}, F2_error_sys=${8}, "`
aCMD=`echo -e "${aCMD} nucleon='p' ;"`

#cmd=${cmd}"E_incident=${IncidentEnergy},scattering_angle=${ElectronAngle},"
#cmd=${cmd}"${quantity}=${3},err_${quantity}=${4},A=${A},Z=${Z},"
mysql nuclearDB -u bjorken -pdrell -N -e"${aCMD}"

done
