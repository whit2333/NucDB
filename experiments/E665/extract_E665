#!/bin/bash
  
# just want 1 2 3 4 5
echo " x   Q**2   F2(R=0)      errors(stat,sys)  F2(R=QCD)   errors "

aCMD=`echo -e "Create table F2_e665 ( \nnucleon varchar(10), \n xbjorken double ,\n Qsquared double ,\n F2 double ,\n F2_error_stat double ,\n f2_error_sys double\n )" `
mysql nuclearDB -u bjorken -pdrell -N -e"${aCMD}"

exec 3< extractors/E665/f2prot.dat
while read <&3 #### LOOP OVER LINES
do  # use $REPLY
  set -- $REPLY
#  echo $1 $2 $3 $4 $5
aCMD=`echo -e "INSERT INTO F2_e665 set "`
aCMD=`echo -e "${aCMD} xbjorken=${3}, "`
aCMD=`echo -e "${aCMD} Qsquared=${4}, "	`
aCMD=`echo -e "${aCMD} F2=${5}, F2_error_stat=${6}, F2_error_sys=${7}, "`
aCMD=`echo -e "${aCMD} nucleon='p' ;"`
#cmd=${cmd}"E_incident=${IncidentEnergy},scattering_angle=${ElectronAngle},"
#cmd=${cmd}"${quantity}=${3},err_${quantity}=${4},A=${A},Z=${Z},"
mysql nuclearDB -u bjorken -pdrell -N -e"${aCMD}"
done
